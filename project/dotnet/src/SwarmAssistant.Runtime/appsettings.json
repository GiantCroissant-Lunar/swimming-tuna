{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Runtime": {
    "Profile": "local",
    "RoleSystem": "akka",
    "AgentExecution": "microsoft-agent-framework",
    "AgentFrameworkExecutionMode": "in-process-workflow",
    "RoleExecutionTimeoutSeconds": 120,
    "CliAdapterOrder": ["copilot", "cline", "kimi", "local-echo"],
    "SandboxMode": "docker",
    "DockerSandboxWrapper": {
      "Command": "",
      "Args": []
    },
    "AppleContainerSandboxWrapper": {
      "Command": "",
      "Args": []
    },
    "AgUiEnabled": true,
    "AgUiBindUrl": "http://127.0.0.1:5080",
    "AgUiProtocolVersion": "0.1",
    "A2AEnabled": false,
    "A2AAgentCardPath": "/.well-known/agent-card.json",
    "ArcadeDbEnabled": false,
    "ArcadeDbHttpUrl": "http://127.0.0.1:2480",
    "ArcadeDbDatabase": "swarm_assistant",
    "ArcadeDbUser": "root",
    "ArcadeDbPassword": "",
    "ArcadeDbAutoCreateSchema": true,
    "LangfuseBaseUrl": "http://localhost:3000",
    "LangfuseTracingEnabled": false,
    "LangfusePublicKey": "",
    "LangfuseSecretKey": "",
    "LangfuseOtlpEndpoint": "http://localhost:3000/api/public/otel/v1/traces",
    "HealthHeartbeatSeconds": 15,
    "AutoSubmitDemoTask": true,
    "DemoTaskTitle": "Phase 8 AG-UI bidirectional action loop",
    "DemoTaskDescription": "Accept tasks via A2A and AG-UI actions, then stream status updates to Godot with ArcadeDB-backed snapshots.",
    "SimulateBuilderFailure": false,
    "SimulateReviewerFailure": false
  }
}
