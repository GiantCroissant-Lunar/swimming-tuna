{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Runtime": {
    "Profile": "local",
    "RoleSystem": "akka",
    "AgentExecution": "microsoft-agent-framework",
    "AgentFrameworkExecutionMode": "in-process-workflow",
    "RoleExecutionTimeoutSeconds": 120,
    "CliAdapterOrder": ["copilot", "cline", "kimi", "kilo", "local-echo"],
    "SandboxMode": "docker",
    "DockerSandboxWrapper": {
      "Command": "",
      "Args": []
    },
    "AppleContainerSandboxWrapper": {
      "Command": "",
      "Args": []
    },
    "AgUiEnabled": true,
    "AgUiBindUrl": "http://127.0.0.1:5080",
    "AgUiProtocolVersion": "0.1",
    "A2AEnabled": false,
    "A2AAgentCardPath": "/.well-known/agent-card.json",
    "ArcadeDbEnabled": false,
    "ArcadeDbHttpUrl": "http://127.0.0.1:2480",
    "ArcadeDbDatabase": "swarm_assistant",
    "ArcadeDbUser": "",
    "ArcadeDbPassword": "",
    "ArcadeDbAutoCreateSchema": true,
    "MemoryBootstrapEnabled": true,
    "MemoryBootstrapLimit": 200,
    "MemoryBootstrapSurfaceLimit": 50,
    "MemoryBootstrapOrderBy": "updated",
    "LangfuseBaseUrl": "http://localhost:3000",
    "LangfuseTracingEnabled": false,
    "LangfusePublicKey": "",
    "LangfuseSecretKey": "",
    "LangfuseOtlpEndpoint": "http://localhost:3000/api/public/otel/v1/traces",
    "HealthHeartbeatSeconds": 15,
    "AutoSubmitDemoTask": false,
    "DemoTaskTitle": "Phase 10 startup memory bootstrap",
    "DemoTaskDescription": "Restore persisted tasks from ArcadeDB into runtime registry at startup and seed Godot surfaces immediately.",
    "SimulateBuilderFailure": false,
    "SimulateReviewerFailure": false,
    "HitlEnabled": false,
    "GraphTelemetryEnabled": false
  }
}
