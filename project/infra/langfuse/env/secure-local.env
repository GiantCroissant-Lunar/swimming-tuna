# Secure local profile (still local machine, stricter defaults)
# WARNING: Replace ALL CHANGE_ME_* values below before using this file.
#          Do NOT commit a copy of this file containing real secrets.
HOST_BIND_IP=127.0.0.1
NEXTAUTH_URL=http://127.0.0.1:3000
LANGFUSE_WEB_PORT=3000
LANGFUSE_WORKER_PORT=3030

POSTGRES_USER=langfuse
# MUST change before use
POSTGRES_PASSWORD=CHANGE_ME_POSTGRES_PASSWORD
POSTGRES_DB=langfuse
# MUST change before use â€” use the same credentials as POSTGRES_USER/PASSWORD above
DATABASE_URL=postgresql://langfuse:CHANGE_ME_POSTGRES_PASSWORD@postgres:5432/langfuse

CLICKHOUSE_USER=langfuse
# MUST change before use
CLICKHOUSE_PASSWORD=CHANGE_ME_CLICKHOUSE_PASSWORD

MINIO_ROOT_USER=langfuse
# MUST change before use
MINIO_ROOT_PASSWORD=CHANGE_ME_MINIO_PASSWORD
LANGFUSE_S3_BUCKET=langfuse
LANGFUSE_MEDIA_S3_ENDPOINT=http://127.0.0.1:9090

# MUST change before use
REDIS_AUTH=CHANGE_ME_REDIS_PASSWORD

# MUST change before use
SALT=CHANGE_ME_RANDOM_SALT
# MUST be a random 64-character hex string before use
ENCRYPTION_KEY=CHANGE_ME_64_CHAR_HEX_KEY
# MUST change before use
NEXTAUTH_SECRET=CHANGE_ME_NEXTAUTH_SECRET
TELEMETRY_ENABLED=false
