{
  "output": {
    "filePath": "repomix-output.xml",
    "style": "xml",
    "removeComments": false,
    "removeEmptyLines": false,
    "showLineNumbers": true,
    "copyToClipboard": false,
    "topFilesLength": 5,
    "headerText": "This repository contains the swimming-tuna project."
  },
  "include": [],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "*.lock",
      "*.lock.yml",
      "*.lock.json",
      "repomix-output.*",
      ".secrets.baseline",
      "build/",
      "bin/",
      "obj/",
      "node_modules/",
      "*.min.js",
      "*.min.css",
      "dist/",
      "coverage/",
      ".vs/",
      ".vscode/",
      "*.user",
      "*.suo"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  }
}
